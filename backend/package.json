{
  "name": "formulario-backend",
  "version": "1.0.0",
  "description": "Backend API para plataforma de formularios",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "migrate": "npx prisma migrate dev",
    "migrate:deploy": "npx prisma migrate deploy",
    "db:push": "npx prisma db push",
    "generate": "npx prisma generate",
    "studio": "npx prisma studio",
    "create-admin": "node src/scripts/createSuperAdmin.js",
    "init-templates": "node src/scripts/initTemplates.js",
    "init-settings": "node src/scripts/initSettings.js",
    "init-all": "npm run create-admin && npm run init-templates && npm run init-settings"
  },
  "keywords": [
    "forms",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.10",
    "pdf-lib": "^1.17.1",
    "sharp": "^0.34.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "prisma": "^6.18.0"
  }
}
